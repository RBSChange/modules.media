<?xml version="1.0"?>
<panel>	
	<section name="properties" linkedtab="properties" labeli18n="modules.uixul.bo.doceditor.section.Properties" image="{HttpHost}/icons/small/edit-properties.png">
		<property name="label" labeli18n="modules.uixul.bo.doceditor.property.Label"/>
		<property name="author" labeli18n="modules.uixul.bo.doceditor.property.Author"/>
		<property name="creationdate" labeli18n="modules.uixul.bo.doceditor.property.Creationdate"/>
	</section>
		
	<section name="content" 
		labeli18n="modules.uixul.bo.doceditor.section.Content" 
		image="{HttpHost}/icons/small/shadow/contract.png">
